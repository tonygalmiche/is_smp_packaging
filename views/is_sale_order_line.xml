<?xml version="1.0" encoding="utf-8"?><openerp><data>        <record id="is_sale_order_line_form_view" model="ir.ui.view">        <field name="name">is_sale_order_line_form_view</field>        <field name="model">is.sale.order.line</field>        <field name="arch" type="xml">            <form>                <sheet>                    <group>                        <group>                            <field name="order_id"/>                            <field name="date_order"/>                            <field name="partner_id"/>                            <field name="state"/>                            <field name="is_affaire_id"/>                            <field name="sequence"/>                            <field name="product_id"/>                            <field name="is_categorie_article_id"/>                            <field name="product_uom_qty"/>                            <field name="qty_delivered"/>                            <field name="qty_invoiced"/>                            <field name="price_unit"/>                            <field name="price_subtotal"/>                            <field name="price_total"/>                        </group>                    </group>                </sheet>            </form>        </field>    </record>    <record id="is_sale_order_line_tree_view" model="ir.ui.view">        <field name="name">is_sale_order_line_tree_view</field>        <field name="model">is.sale.order.line</field>        <field name="arch" type="xml">            <tree>                <field name="date_order"/>                <field name="order_id"                widget="many2one_clickable"/>                <field name="partner_id"              widget="many2one_clickable"/>                <field name="state"/>                <field name="is_affaire_id"           widget="many2one_clickable"/>                <field name="product_id"              widget="many2one_clickable"/>                <field name="is_categorie_article_id" widget="many2one_clickable"/>                <field name="product_uom_qty" sum="Total"/>                <field name="qty_delivered"   sum="Total"/>                <field name="qty_invoiced"    sum="Total"/>                <field name="price_unit"/>                <field name="price_subtotal"  sum="Total"/>            </tree>        </field>    </record>    <record id="is_sale_order_line_search_view" model="ir.ui.view">        <field name="name">is_sale_order_line_search_view</field>        <field name="model">is.sale.order.line</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="order_id"/>                <field name="partner_id"/>                <field name="state"/>                <field name="is_affaire_id"/>                <field name="product_id"/>                <field name="is_categorie_article_id"/>                <group expand="1" string="Regrouper par...">                    <filter string="Commande "    context="{'group_by':'order_id'}"/>                    <filter string="Fournisseur " context="{'group_by':'partner_id'}"/>                    <filter string="Article"      context="{'group_by':'product_id'}"/>                    <filter string="CatÃ©gorie"    context="{'group_by':'is_categorie_article_id'}"/>                    <filter string="Machine"      context="{'group_by':'is_affaire_id'}"/>                </group>            </search>        </field>    </record>    <record id="is_sale_order_line_pivot_view" model="ir.ui.view">        <field name="name">is_sale_order_line_pivot_view</field>        <field name="model">is.sale.order.line</field>        <field name="arch" type="xml">            <pivot string="Lignes de commandes">                <field name="partner_id"              type="row"/>                <field name="is_categorie_article_id" type="col"/>                <field name="price_subtotal"          type="measure"/>            </pivot>        </field>    </record>    <record id="is_sale_order_line_graph_view" model="ir.ui.view">        <field name="name">is_sale_order_line_graph_view</field>        <field name="model">is.sale.order.line</field>        <field name="arch" type="xml">            <graph string="Lignes de commandes">                <field name="partner_id"              type="row"/>                <field name="price_subtotal"          type="measure"/>            </graph>        </field>    </record>    <record id="is_sale_order_line_action" model="ir.actions.act_window">        <field name="name">Lignes de commandes</field>        <field name="res_model">is.sale.order.line</field>        <field name="view_type">form</field>        <field name="view_mode">tree,form,pivot,graph</field>    </record></data></openerp>