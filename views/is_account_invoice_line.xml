<?xml version="1.0" encoding="utf-8"?><openerp><data>    <record id="is_account_invoice_line_form_view" model="ir.ui.view">        <field name="name">is_account_invoice_line_form_view</field>        <field name="model">is.account.invoice.line</field>        <field name="arch" type="xml">            <form>                <sheet>                    <group>                        <group>                            <field name="number"/>                            <field name="invoice_id"/>                            <field name="date_invoice"/>                            <field name="date_due"/>                            <field name="partner_id"/>                            <field name="product_id"/>                            <field name="description"/>                            <field name="is_affaire_id"/>                            <field name="quantity"/>                            <field name="price_unit"/>                            <field name="price_subtotal"/>                            <field name="type"/>                            <field name="state"/>                        </group>                    </group>                </sheet>            </form>        </field>    </record>    <record id="is_account_invoice_line_tree_view" model="ir.ui.view">        <field name="name">is_account_invoice_line_tree_view</field>        <field name="model">is.account.invoice.line</field>        <field name="arch" type="xml">            <tree>                <field name="number"/>                <field name="date_invoice"/>                <field name="date_due"/>                <field name="partner_id"/>                <field name="description"/>                <field name="is_affaire_id"/>                <field name="quantity" sum="Total"/>                <field name="price_unit"/>                <field name="price_subtotal" sum="Total"/>                <field name="type"/>                <field name="state"/>            </tree>        </field>    </record>    <record id="is_account_invoice_line_search_view" model="ir.ui.view">        <field name="name">is_account_invoice_line_search_view</field>        <field name="model">is.account.invoice.line</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="number"/>                <field name="invoice_id"/>                <field name="partner_id"/>                <field name="product_id"/>                <field name="description"/>                <field name="is_affaire_id"/>                <field name="type"/>                <field name="state"/>            </search>        </field>    </record>    <record id="is_account_invoice_line_pivot_view" model="ir.ui.view">        <field name="name">is_account_invoice_line_pivot_view</field>        <field name="model">is.account.invoice.line</field>        <field name="arch" type="xml">            <pivot string="Lignes de commandes">                <field name="is_affaire_id"           type="row"/>                <field name="state"                   type="col"/>                <field name="price_subtotal"          type="measure"/>            </pivot>        </field>    </record>    <record id="is_account_invoice_line_graph_view" model="ir.ui.view">        <field name="name">is_account_invoice_line_graph_view</field>        <field name="model">is.account.invoice.line</field>        <field name="arch" type="xml">            <graph string="Lignes de commandes">                <field name="is_affaire_id"           type="row"/>                <field name="price_subtotal"          type="measure"/>            </graph>        </field>    </record>    <record id="is_account_invoice_line_action" model="ir.actions.act_window">        <field name="name">Lignes de factures</field>        <field name="res_model">is.account.invoice.line</field>        <field name="view_type">form</field>        <field name="view_mode">tree,form,pivot,graph</field>        <field name="domain">['|',('type','=', 'in_invoice'),('type','=', 'in_refund')]</field>    </record></data></openerp>